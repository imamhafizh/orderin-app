<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.yustian.student.orderin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yustian.student.orderin.MainActivity"></activity>
        <activity android:name="com.yustian.student.orderin.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yustian.student.orderin.ProfileActivity"></activity>
        <activity android:name="com.yustian.student.orderin.CreateActivity"></activity>
        <activity android:name="com.yustian.student.orderin.DetailActivity"></activity>
    </application>

</manifest>